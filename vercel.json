{
  "version": 2,
  "name": "stellar-walrus-glow",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "pnpm install --no-frozen-lockfile && pnpm add @rollup/rollup-linux-x64-gnu"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)", 
      "dest": "/"
    }
  ]
}